@model IEnumerable<Frontend.Models.Data.ViewModel.UserRoleViewModel>

@{
    ViewData["Title"] = "GetAdministrators";
    Layout = "~/Views/Shared/_AdminDashboardLayout.cshtml";
}



<div class="main-content">
    <div class="section__content section__content--p30">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">
                    <h1>List of Agric Extension Officers</h1>
                    <br />
                    <div class="card shadow">
                        <div class="card-body">
                            <table id="tblOfficer" class="table table-hover table-sm table-bordered">
                                <thead>
                                    <tr>
                                        <th>
                                           Name
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.Contact)
                                        </th>
                                        <th>
                                           Role
                                        </th>

                                        <th></th>
                                    </tr>
                                </thead>

                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


@section Scripts{

    <script src="~/js/Tables/GetOfficersList.js"></script>
    <script src="~/lib/datatables/js/jquery.dataTables.js"></script>
    <script src="~/lib/datatables/js/dataTables.bootstrap4.js"></script>
    <script src="~/lib/sweetalert/sweetalert.min.js"></script>


}
